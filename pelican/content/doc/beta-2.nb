 RowBox[{
  RowBox[{
   RowBox[{"x", "+", 
    SubscriptBox["x", "1"]}], "\[GreaterEqual]", 
   RowBox[{"2", "m"}]}], " ", "\[Implies]", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"x", " ", "+", " ", 
     RowBox[{
      SubsuperscriptBox["f", "2", 
       RowBox[{"-", "1"}]], "[", 
      RowBox[{"f", "[", "x", "]"}], "]"}]}], "\[GreaterEqual]", 
    RowBox[{"2", "m"}]}], "\[Implies]", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["f", "2", 
      RowBox[{"-", "1"}]], "[", 
     RowBox[{"f", "[", "x", "]"}], "]"}], "\[GreaterEqual]", 
    RowBox[{
     RowBox[{"2", "m"}], "-", "x"}]}]}]}]], "DisplayFormula",
