       RowBox[{"Sqrt", "[", "b", "]"}]}], ")"}], "/", 
     RowBox[{"Sqrt", "[", "c", "]"}]}], ",", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Sqrt", "[", "b", "]"}], "*", 
       RowBox[{"Sqrt", "[", "c", "]"}]}], ")"}], "/", 
     RowBox[{"Sqrt", "[", "a", "]"}]}]}], "}"}]}]], "Input",
